## Misc scripts PATH
export MICSPATH=/home/ww/WW/Misc/
export PATH=${MICSPATH}:$PATH
for subfolder in $(ls $MICSPATH); do
    if test -d "${MICSPATH}/${subfolder}"; then
        export PATH=${MICSPATH}/${subfolder}/:$PATH
    fi
done

